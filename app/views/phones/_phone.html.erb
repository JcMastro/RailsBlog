<table>
    <thead>
        <tr>
        <th>Phone type</th>
        <th>Number</th>        
        <th colspan= "3"></th>
        </tr>
    </thead>
    <tbody>
        <% @contact.phones.each do |phone| %>
            <tr>    
                <td> <%= phone.phone_type %></td>
                <td><%= phone.number%></td>
                <td><%= link_to 'Edit', edit_contact_phone_path(@contact)%> </td>
                <td><%= link_to 'Destroy', method: :delete, data: {confirm: 'esta seguro?'}%> </td>
            </tr>

        <% end %>
    </tbody>
</table>
    
